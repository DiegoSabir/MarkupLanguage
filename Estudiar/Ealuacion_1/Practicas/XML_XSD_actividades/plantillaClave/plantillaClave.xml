<?xml version="1.0" encoding="UTF-8"?>
<!-- Plantilla para implementar claves -->
<plantillaClave xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:noNamespaceSchemaLocation="plantillaClave.xsd">
  <elementoDependiente nombreDependiente="Madrid" atributoDependiente="ES"/>
  <elementoDependiente nombreDependiente="Tokio" atributoDependiente="JP"/>

  <elementoIndependiente atributoIndependiente="ES">
    <nombreIndependiente>España</nombreIndependiente>
  </elementoIndependiente>
  <elementoIndependiente atributoIndependiente="JP">
    <nombreIndependiente>Japón</nombreIndependiente>
  </elementoIndependiente>
  
<!-- el siguiente elementoIndependiente puede existir sin ser referenciado -->  
  <elementoIndependiente atributoIndependiente="PT">
    <nombreIndependiente>Portugal</nombreIndependiente>
  </elementoIndependiente>
 
</plantillaClave>
